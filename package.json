{
  "name": "kevintamakuwala-portfolio",
  "description": "My portfolio built using Next.js",
  "version": "6.0.0",
  "author": "Kevin Tamakuwala",
  "homepage": "http://localhost:3000",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "dev:turbo": " next dev --turbo --show-all",
    "build": "next build",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier . --write --cache --ignore-unknown --plugin=prettier-plugin-tailwindcss",
    "format:check": "prettier . --check --cache --ignore-unknown",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@headlessui/react": "2.2.0",
    "@headlessui/tailwindcss": "0.2.1",
    "@vercel/analytics": "1.3.2",
    "clsx": "2.1.1",
    "contentlayer2": "0.5.1",
    "dotenv": "16.4.5",
    "eslint-config-next": "^15.0.2",
    "geist": "1.3.1",
    "github-slugger": "2.0.0",
    "lucide-react": "0.454.0",
    "mdx-bundler": "10.0.3",
    "next": "15.0.2",
    "next-contentlayer2": "0.5.1",
    "next-nprogress-bar": "2.3.14",
    "next-themes": "0.3.0",
    "nprogress": "0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "7.1.0",
    "rehype-pretty-code": "0.14.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "4.0.0",
    "server-only": "0.0.1",
    "sharp": "0.33.5",
    "tailwind-merge": "2.5.4",
    "validator": "13.12.0",
    "vaul": "1.1.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.15",
    "@types/node": "22.8.2",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0",
    "@types/validator": "13.12.2",
    "autoprefixer": "10.4.20",
    "eslint": "9.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globby": "14.0.2",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "tailwind-gradient-mask-image": "1.2.0",
    "tailwindcss": "3.4.14",
    "tailwindcss-text-fill": "0.2.0",
    "typescript": "5.6.3"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "packageManager": "pnpm@9.12.3",
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0"
    }
  }
}
